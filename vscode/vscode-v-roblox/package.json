{
    "name": "vscode-v-roblox",
    "displayName": "VSCode-v-Roblox",
    "description": " A more simplistic way to program your Roblox game's scripts in Visual Studio Code. ",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.78.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vscode-v-roblox.start",
                "title": "VSCode-v-Roblox: Start the connection between VSCode and Roblox."
            },
            {
              "command": "vscode-v-roblox.stop",
              "title": "VSCode-v-Roblox: Stop the connection between VSCode and Roblox."
          }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.78.0",
        "@vscode/test-electron": "^2.3.0",
        "eslint": "^8.39.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "express": "^4.18.2"
    }
}
